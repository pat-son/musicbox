<% provide(:title, @creation.name) %>
<%= javascript_tag do %>
    window.data = JSON.parse('<%= raw @creation.data %>')
<% end %>

<input id="creation-name" class="form-control" name="name" type="text" placeholder="Title" value="<%= @creation.name %>">
<button id="save-creation" class="btn btn-primary">Save</button>

<div id="create-song">
  <%= render "toolbar" %>
  <%= render "note_board" %>
</div>
